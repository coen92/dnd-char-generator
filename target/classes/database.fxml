<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.TableView?>


<AnchorPane xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.coen92.projects.dndchargenerator.control.DatabaseController">
    <children>
        <TableView fx:id="charDataTable" AnchorPane.bottomAnchor="15" AnchorPane.topAnchor="15"
                   AnchorPane.leftAnchor="10" AnchorPane.rightAnchor="10">
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
    </children>
</AnchorPane>